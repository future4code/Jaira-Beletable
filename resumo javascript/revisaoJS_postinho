function validarSegundaDose(nome) {
    const usuarios = [
       { nome: "Artur", imunizacao: "incompleta" },
       { nome: "Barbara", imunizacao: "incompleta" },
       { nome: "Carlos", imunizacao: "incompleta" }
]
for(let usuarios of usuarios) {
    if(usuarios.nome === "Artur" && usuarios.nome === "Barbara" && usuarios.nome === "Carlos")
     usuarios.imunizacao === completo
}


}validarSegundaDose(nome)

exercicio 7

function checarSegundaDose(nome,imunizacao) {
  const usuarios = [
       { nome: "Artur", imunizacao: "incompleta" },
      { nome: "Barbara", imunizacao: "completa" },
       { nome: "Carlos", imunizacao: "incompleta" }
]
  for(usuarios of usuarios) {
   if(usuarios.inumizacao === "incompleta") {
    console.log(Olá ${usuarios.nome}! Sua imunização está ${usuarios.imunizacao}, por favor volte ao postinho para tomar a segunda dose.`)   
    }

   }
}checarSegundaDose(nome,imunizacao)